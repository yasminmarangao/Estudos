algoritmo "Exerc.53"
var
    comp, l, preco: real
inicio
    escreva("Escreva a dimensão de comprimento do terreno: ")
    leia(comp)
    escreva("Escreva a dimensão de largura do terreno: ")
    leia(l)
    escreva("Digite o preço do metro de tela: ")
    leia(preco)
    escreva("O custo para telar ", comp, " metros ", " e", l, " metros ", "é igual a: ", ((comp*l)*preco))
fimalgoritmo

algoritmo "Exerc.39"
var
    impor: real
inicio
    escreval("A importância de 780.000,00 reais será dividida entre os ganhador do concurso")
    impor <- 780000
    escreval("O primeiro ganhador receberá: ", "R$",(0.46*impor)," reais")
    escreval("O segundo ganhador receberá: ", "R$",(0.32*impor)," reais")
    escreva("O terceiro ganhador receberá: ", "R$",(0.22*impor)," reais")
fimalgoritmo

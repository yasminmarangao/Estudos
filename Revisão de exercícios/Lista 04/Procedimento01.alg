algoritmo "semnome"
var
    num1, num2: inteiro
    
procedimento soma(x,y: inteiro)

inicio
    escreva("A soma entre os dois números é: ", (x+y))
fimprocedimento
inicio
    escreva("Digite um número: ")
    leia(num1)
    escreva("Digite outro número: ")
    leia(num2)
    soma(num1,num2)
fimalgoritmo

algoritmo "Exerc.1"
var
    num1, num2: inteiro
inicio
    escreva("Digite o primeiro numero: ")
    leia(num1)
    limpatela()
    escreva("Digite o segundo numero: ")
    leia(num2)
    se(num1 > num2) entao
        escreva("O primeiro número é maior que o segundo número")
    senao
        escreva("O segundo número é maior que o primeiro número")
    fimse
fimalgoritmo

algoritmo "Algoritmo01"
var
   num1, num2: real
   
inicio
   escreva("Digite o primeiro numero: ")
   leia(num1)
   escreva("Digite o segundo numero: ")
   leia(num2)
   
   se(num1 > num2) entao
      escreva("O maior numero eh: ", num1)
   senao
     escreva("O maior numero eh: ", num2)
     
   fimse
fimalgoritmo

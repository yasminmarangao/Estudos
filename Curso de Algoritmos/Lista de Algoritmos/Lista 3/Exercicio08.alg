algoritmo "Algoritmo08"
var
   cont, limite, salto: inteiro
inicio
   cont <- 0

   escreva("Digite até quanto vai o limite: ")
   leia(limite)
   escreva("Digite o valor do salto: ")
   leia(salto)

   enquanto cont <= limite faca
      escreva(cont)
      cont <- cont + salto
   fimenquanto
fimalgoritmo

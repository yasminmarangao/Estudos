algoritmo "PROCEDIMENTOS COM PARÂMETROS"
var
   x, y: inteiro
procedimento NomeProcedimento(A, B: tipo)

inicio
   //bloco de comando
	escreval(A)  // escrevendo o valor da variável A passado por valor de referência
	escreval(B)  // escrevendo o valor da variável B passado por valor de referência

fimprocedimento

inicio
   x <- valor
   y <- valor
   NomeProcedimento(x, y)
fimalgoritmo
